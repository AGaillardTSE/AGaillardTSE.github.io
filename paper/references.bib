	Institution = {Department of Economics, UC Santa Cruz},
	Keywords = {Business; Social and Behavioral Sciences; entrepreneurship; business; startups; self-employment},
	Title = {The Kauffman Index: Startup Activity | National Trends},
	Type = {Santa Cruz Department of Economics, Working Paper Series},
