lorenz <- function(data, data2) {
cumulative = c()
totalwealth = c()
cumulativewealth = c()
cumulativewealth[1] = data2[1]*data[1]
cumulative[1] = data[1]
size=length(data)
for(i in 1:size) {
totalwealth[i] = data2[i]*data[i]
}
totalw = sum(totalwealth)
for(i in 2:size) {
cumulative[i] = cumulative[i-1] + data[i]
}
for(i in 2:size) {
cumulativewealth[i] = cumulativewealth[i-1] + data2[i]*data[i]
}
cumulativew = cumulativewealth/totalw
lorenzpoint = cbind(cumulative, cumulativew)
}
